<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core" xmlns:charts="sap.suite.ui.microchart" xmlns:camera="sap.challenge.mtshelf.mtshelf_web.control">
	<Dialog title="Verify Picture of Missing-Tag Item" contentWidth="400px" contentHeight="300px" horizontalScrolling="false"
		verticalScrolling="false" afterClose="onAfterUploadClosed">
		<buttons>
			<Button text="Upload Image" press=".onUploadImage"/>
			<Button text="Cancel" press="onCancelUpload"/>
		</buttons>
		<content>
			<FlexBox alignItems="Center" justifyContent="Center">
				<Image src="{/photos/0/src}" densityAware="false" decorative="false" height="320px" width="240px"></Image>
			</FlexBox>
		</content>
	</Dialog>
</core:FragmentDefinition>